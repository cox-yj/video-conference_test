{
  "name": "video-conference",
  "version": "0.1.0",
  "description": "A video conferencing app built with React, MobX and TypeScript",
  "scripts": {
    "start": "cross-env NODE_ENV=development BASE_ENV=development webpack serve -c webpack/webpack.dev.ts",
    "build": "cross-env NODE_ENV=production BASE_ENV=production webpack -c webpack/webpack.prod.ts",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint-staged": "lint-staged --allow-empty",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx ",
    "prepare": "husky install"
  },
  "dependencies": {
    "@ant-design/icons": "^5.0.1",
    "antd": "^4.16.11",
    "classnames": "^2.3.2",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "i18next": "22.4.10",
    "mobx": "^6.3.3",
    "mobx-react": "^7.2.0",
    "mobx-react-lite": "3.4.0",
    "rc-select": "^14.2.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-i18next": "12.2.0",
    "react-router-dom": "6.8.1",
    "react-scripts": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.21.0",
    "@babel/preset-env": "^7.20.2",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.21.0",
    "@types/jest": "^27.4.0",
    "@types/node": "^16.18.13",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@types/webpack": "^5.28.0",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "babel-loader": "^9.1.2",
    "core-js": "^3.29.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.3",
    "eslint": "^8.9.0",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.3.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.0",
    "jest": "^27.4.7",
    "less": "^4.1.2",
    "less-loader": "^10.2.0",
    "lint-staged": "^13.1.2",
    "prettier": "^2.8.4",
    "style-loader": "^3.3.1",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-merge": "^5.8.0"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": "npm run lint-staged:js",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
